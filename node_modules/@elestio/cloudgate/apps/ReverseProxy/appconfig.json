{
  "env": "PROD",
  "version": "1.0.0",
  "title": "Reverse Proxy App",
  "description": "This is a sample app using reverse proxy endpoints",
  "domains": ["*"], 
  "publicFolder": "./public",
  "TypeAPI": "LOCAL",
  "TypeFS": "LOCAL",
  "apiEndpoints": {
    "/local" : {
      "src": "./API/",
      "handler": "home.handler"
    },
    "/*" : {
      "skipPrefix": false,
      "src": "https://www.google.com/",
      "postProcessor": "./API/rewriter.js",
      "handler": "rewriter.handler"
    }
  },
  "websocketEndpoints": {
    "/*" : {
      "src" : "https://www.google.com/",
      "postProcessor": "./API/rewriter.js",
      "upgrade": "rewriter.upgrade",
      "open": "rewriter.open",
      "message": "rewriter.message",
      "close": "rewriter.close"
    }
  }
}