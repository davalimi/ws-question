{
    "SETTINGS": {},
    "CLUSTER": {},
    "GLOBAL": {
        "AdminConfig": {
            "watch": false,
            "debug": false,
            "nbThreads": 12
        },
        "*": {
            "env": "PROD",
            "version": "1.0.0",
            "title": "ws-monitoring",
            "description": "This is a sample app that handle server monitoring",
            "domains": [
                "*"
            ],
            "publicFolder": "./public",
            "TypeAPI": "LOCAL",
            "TypeFS": "LOCAL",
            "redirect404toIndex": false,
            "HSTS": false,
            "maxRequestsPerMinutePerIP": 100,
            "CORS": {
                "access-control-allow-origin": "*"
            },
            "apiEndpoints": {},
            "websocketEndpoints": {
                "/monitoring": {
                    "src": "./API/websocket/",
                    "open": "Monitoring.open",
                    "message": "Monitoring.message",
                    "close": "Monitoring.close",
                    "token": "XXXXXXXXXXXXXXXXXXXXXXXXXX"
                },
                "/chat": {
                    "src": "./API/websocket/",
                    "open": "Chat.open",
                    "message": "Chat.message",
                    "close": "Chat.close"
                }
            },
            "root": "/Users/david/Projects/ws-monitoring/"
        }
    }
}